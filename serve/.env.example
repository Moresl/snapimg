# 应用配置
APP_NAME=Advanced Image Compressor
APP_VERSION=1.0.0
DEBUG=True

# 文件上传配置
MAX_FILE_SIZE=52428800  # 50MB (bytes)
MAX_FILES_PER_BATCH=100

# 存储配置
UPLOAD_DIR=uploads
COMPRESSED_DIR=compressed
HISTORY_DIR=history

# 压缩配置
DEFAULT_QUALITY=85
USE_PNGQUANT=True

# CORS 配置
CORS_ORIGINS=*

# 数据库配置
DATABASE_URL=sqlite:///./compress_history.db
